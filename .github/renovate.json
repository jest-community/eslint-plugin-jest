{
  "extends": ["config:recommended", "helpers:disableTypesNodeMajor"],
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true
  },
  "rangeStrategy": "replace",
  "postUpdateOptions": ["yarnDedupeHighest"],
  "packageRules": [
    {
      "matchFileNames": [".github/**"],
      "groupName": "workflows"
    },
    {
      "matchDepTypes": ["uses-with"],
      "enabled": false
    }
  ]
}
